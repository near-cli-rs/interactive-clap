impl interactive_clap::ToCliArgs for Mode {
    fn to_cli_args(&self) -> std::collections::VecDeque<String> {
        match self {
            Self::Network => {
                let mut args = std::collections::VecDeque::new();
                args.push_front("network".to_owned());
                args
            }
            Self::Offline => {
                let mut args = std::collections::VecDeque::new();
                args.push_front("offline".to_owned());
                args
            }
        }
    }
}
